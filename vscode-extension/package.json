{
    "name": "code4arm",
    "publisher": "ondryaso",
    "displayName": "Code For Arm",
    "description": "Arm Unified Assembly Language support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "dependencies": {
        "vscode-languageclient": "^7.0.0",
        "net": "^1.0.2"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./out/main.js",
    "contributes": {
        "languages": [
            {
                "id": "arm-ual",
                "aliases": [
                    "Arm UAL",
                    "arm-ual"
                ],
                "extensions": [
                    ".s",
                    ".S"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "arm-ual",
                "scopeName": "source.armual",
                "path": "./syntaxes/arm-ual.tmLanguage.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:arm-ual"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@types/vscode": "^1.63.0",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "@vscode/test-electron": "^2.0.3",
        "eslint": "^8.6.0",
        "glob": "^7.2.0",
        "mocha": "^9.1.3",
        "typescript": "^4.5.4"
    }
}
